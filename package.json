{
  "name": "kattwalksalon",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel index.pug",
    "watch": "parcel watch *.html *.pug",
    "build": "rm -rf dist .cache && parcel build *.html *.pug --no-minify"
  },
  "dependencies": {},
  "devDependencies": {
    "@prettier/plugin-pug": "^1.4.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.13",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "pug": "^3.0.1",
    "tailwindcss": "^1.4.6"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      "images": [
        ".jpg",
        ".png",
        ".webp"
      ],
      ".css": "css"
    }
  },
  "browserslist": [
    "> 0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": "*.pug",
        "options": {
          "parser": "pug"
        }
      }
    ]
  },
  "license": "ISC"
}
